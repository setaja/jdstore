
<h1>add a stuff </h1>
<%= simple_form_for @good do |f| %>
<%= f.input :title %>
<%= f.input :description %>
<%= f.input :stuff_size, :label => "尺寸" %>
<%= f.input :stuff_price, :label => "價格" %>
<%= f.input :stuff_quantity, :label => "庫存數量" %>
<%= f.input :is_hidden %>
  <%= f.submit "Submit" %>
<% end %>
